<?xml version='1.0' encoding='utf-8'?>
<widget id="com.myplaceonline" version="1.0.10" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>myplaceonline</name>
    <description>myplaceonline is a joy and meaning maximizer. https://myplaceonline.com/</description>
    <author email="contact@myplaceonline.com" href="https://myplaceonline.com/">contact@myplaceonline.com</author>
    <content src="index.html" />
    <icon src="icon.png" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <access origin="*" />
    <gap:config-file parent="/manifest" platform="android">
        <application android:debuggable="true" android:theme="@android:style/Theme.NoTitleBar" />
    </gap:config-file>
    <preference name="phonegap-version" value="cli-5.4.1" />
    <preference name="orientation" value="default" />
    <preference name="fullscreen" value="false" />
    <preference name="permissions" value="none" />
    <preference name="KeepRunning" value="true" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
</widget>
